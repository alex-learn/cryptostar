{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "prebuild": "npm run build-css",
    "build": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "clean": "rm -rf build",
    "eject": "react-scripts eject",
    "start": "concurrently  \"npm run watch-css\" \"npm run start-js\" ",
    "start-js": "PORT=8080 react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && npm run build-css --watch --recursive"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "binstring": "0.2.1",
    "blueimp-md5": "2.10.0",
    "fingerprintjs2": "git+https://github.com/Valve/fingerprintjs2.git",
    "gun": "0.9.2",
    "gun-unset": "1.0.1",
    "ipaddr.js": "1.5.4",
    "js-cookie": "2.2.0",
    "le_js": "git+https://github.com/rapid7/le_js.git",
    "levelup": "2.0.1",
    "localstorage-down": "0.6.7",
    "lokka": "1.7.0",
    "lokka-transport-http": "1.6.1",
    "ory-editor": "0.2.25",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-addons-update": "15.6.2",
    "react-dom": "16.2.0",
    "react-md": "1.2.8",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17",
    "react-springy-parallax": "^1.2.0",
    "react-tap-event-plugin": "3.0.2",
    "redux": "3.7.2",
    "redux-effects-universal-cookie": "1.0.1",
    "redux-saga": "0.16.0",
    "secure-ls": "1.1.0",
    "serialize-javascript": "1.4.0",
    "shuffled-priority-queue": "1.0.0",
    "simple-crypto-js": "1.1.1",
    "stackdriver-errors-js": "0.3.0",
    "stacktrace-js": "2.0.0",
    "uuid": "3.1.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "concurrently": "3.5.1",
    "node-sass-chokidar": "0.0.3",
    "react-scripts": "1.0.17",
    "source-map-explorer": "1.5.0"
  }
}
